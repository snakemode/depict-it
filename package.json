        "node-fetch": "^2.6.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.4",
        "@babel/core": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "jest": "^26.1.0",
        "jest-cli": "^26.1.0",
        "live-server": "^1.2.1",
